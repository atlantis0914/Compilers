int bitstoint(bool[] arr, int n)
{
	int i;
	int agg = 0;
	for (i = n - 1; i >= 0; i--)
	{
		agg <<= 1;
		agg += 1 & (arr[i] ? 1 : 0);
	}
	return agg;
}
int inttobits(bool[] arr, int n)
{
	int i = 0;
	while (n != 0)
	{
                arr[i] = ((n & 1) != 0 ? true : false);
		n >>= 1;
		i++;
	}
	while (i < 32) //Necesary due to refcompiler sillies
	{
		arr[i] = false;
		i++;
	}
	return 0;
}

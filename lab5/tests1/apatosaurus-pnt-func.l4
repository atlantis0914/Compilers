//test return 50
// dereference through functions

int derefd (int p) {
    return p;
}
int derefc (int * p) {
    return derefd(*p);
}
int derefb (int ** p) {
    return derefc(*p);
}
int derefa (int *** p) {
    return derefb(*p);
}
int main () {
  int *** p;
  p = alloc(int **);
  *p = alloc (int *);
  **p = alloc (int);
  ***p = 50;
  
  return derefa(p);
}

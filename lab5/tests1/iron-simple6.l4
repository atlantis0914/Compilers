// median value of sorted array

int median(int [] arr, int n) {
		return arr[n/2];
}

int main() {
		int n = 9;
		int[] arr = alloc_array(int, n);
		arr[0] = 3;
		arr[1] = 4;
		arr[2] = 6;
		arr[3] = 8;
		arr[4] = 10;
		arr[5] = 23;
		arr[6] = 32;
		arr[7] = 43;
		arr[8] = 400;
		return median(arr, n);
}
// compute the derivative of a polynomial

typedef int[] poly;

int sum(int[] A, int l) {
  int acc = 0;
  for (int i=0; i<l; i++) {
    acc += A[i];
  }
  return acc;
}

poly derivative(poly p, int d) {
  poly q = alloc_array(int, d-1);
  for (int i = 1; i<d-1; i++) {
    q[i-1] = p[i] * i;
  }
  return q;
}

int main() {
  poly p = alloc_array(int, 256);
  for (int i = 0; i < 256; i++) {
    p[i] = (1664525 * i) + 1013904223; // LCG "random" number
  }
  poly q = derivative(p,256);
  return sum(q,255);
}
//test return 20

int main()
{
    int x;
    x = 5;

    while(x < 100)
    {
        if (x == 0)
            return 20;
        else if (x == 1)
            x = 3;
        else if (x == 2)
        {
            x--;
            x--;
        }
        else if (x == 3)
            x = x + 1;
        else if (x == 4)
            x = x / 2;
        else if (x == 5)
            x = x - 4;
        else
            x = x / 3;
    }

    return x;
}

/* Test successful tracing of initialization in variables across multiple control flow paths */

int main () {
  int x = 63728125;
  int max = x;
  int steps = 0;
  
  while (x != 1) {
    if (x % 2 == 0) {
      x /= 2;
    } else {
      x = 3*x + 1;
    }
    if (x > max)
      max = x;
    steps++;
  }
  
  return max + steps;
}

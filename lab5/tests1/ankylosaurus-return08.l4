bool isPrime(int n){
    int i;
    for(i = 2; i < n; i++){
    	  if(n%i == 0) return false;

    }
    return true;
}

int  main()
{
	int i;
	int count = 0;
        bool done = false;
	for(i = 2; !done && i < 100; i++){
            if(isPrime(i)) count++;
            if(count == 7) done = true;

        }
        return i-1;

}
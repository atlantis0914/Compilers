//test return -1

int main ()
{
  int x = -1;
  
  while(x > 0){
      x++;  // 1
      x <<= x; // 2
      x <<= 33; // 4
      x >>= 1; // 2
      x = ~x; // -3
      x >>= 33; // -2
      x |= 1; // -1
      x &= 1 << 31; // -2^31
      x ^= 1 << 31 >> 31; // 2^31-1
      x = ~ ~ (x);
  }
  return x;
}

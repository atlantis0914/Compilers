//test return 100

int main ()
{
  int n;
  int sum;
  int i;
  int j;

  n = 3;
  sum = 0;
  for (i = 1; i <= n; i+=1) {
    for (j = 1; j <= n; j+=1) {
      sum = sum + i*j;
      if (i != j) {}	  // continue inner loop, not outer
      else sum = sum + i*j;  // count diag twice
    }
  }
  for (i = 1; i <= n; i+=1) {
    j = 1;
    while (j <= n) {
      sum = sum + i*j;
      j += 1;
      if (i+1 != j) {}	// continue inner loop, not outer
      else sum = sum + i*(j-1);	// count diag twice
    }
  }

  return sum;
}

int maxContSeqVal( int [] arr, int len)
{
  int [] m = alloc_array(int, len);
  int i;
  int max = -2147483648;

  m[0] = arr[0];

  for( i = 1; i < len; i++) {
    if( (m[i-1] + arr[i]) > arr[i] ){
      m[i] = m[i-1] + arr[i];
    } else {
      m[i] = arr[i];
    }
  }

  for( i = 0; i < len; i++) {
    if ( m[i] > max ) { max = m[i]; }
  }
  
  return max;
}

int main()
{
  int len = 10;
  int[] arr = alloc_array(int, len);

  arr[0] = -1;
  arr[1] = 3;
  arr[2] = -9;
  arr[3] = 10;
  arr[4] = 3;
  arr[5] = -1;
  arr[6] = 5;
  arr[7] = -10;
  arr[8] = 4;
  arr[9] = -100;

  return maxContSeqVal(arr, len);
}







int gcd(int x, int y) {
	if (x == y) {
		return x;
	}
	if (y < x) {
		return gcd(x-y,y);
	} else {
		return gcd(x,y-x);
	}
}

bool fac(int x) {
	if (x == 1) {
		return x;
	} else {
		return x * fac(x-1);
	}
}

int re(int rem, int acc) {
	if (acc == 0) {
		return rem;
	} else {
		re(rem-1, fac(rem) * gcd(acc,acc*3+1));
	}
}

int main() {
	return gcd(134248, 267);
}

// summing an array

int sum(int [] a, int n) {
		int sum = 0;
		for(int i = 0; i < n; i++) { sum += a[i]; }		
		return sum;
}

int main() {
		int [] arr;
		int n = 100;
		arr = alloc_array(int, n);
		for(int i = 0; i < n; i++) { arr[i] = i; }
		return sum(arr, n);
}


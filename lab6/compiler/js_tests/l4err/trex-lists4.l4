//test exception

//resuts in non termination

struct list_rep {
  int head;
  struct list_rep* tail;
};

typedef struct list_rep list;

int max(list* L)
{
  int t1;
  int t2;
  if (L == NULL) return -1;
  if (L->tail == NULL) return L->head;
  t1 = L->head;
  t2 = max(L->tail);
  return t1 > t2 ? t1 : t2;
}

int main()
{
  list* L1;
  list* L2;
  list* L3;
  L1 = alloc(list);
  L2 = alloc(list);
  L3 = alloc(list);
  L1->head = 1;
  L1->tail = L2;
  L2->head = 2;
  L2->tail = L3;
  L3->head = 3;
  L3->tail = L3;  //circular list
  return max(L1);
}

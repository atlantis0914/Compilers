//test return 6
//returns the length of the longest subsequence (all 1's or all 0's)

int main() {
    int n = 0xF03ABCDE;
    int prev = n & 0x1;
    int max_count = 1;
    int count = 1;
    int i;

    for (i = 1; i < 32; i++) {
        if ((n & 0x1) == prev) {
            count++;
        }
        else {
            if (count > max_count) max_count = count;
            count = 1;
            prev = n & 0x1;
        }
        n >>= 1;
    }

    return (count > max_count) ? count : max_count;
}

// I'MMA LIST

struct list
{
    int data;
    struct list * next;
};

int main()
{
    struct list * l0 = alloc(struct list);
    struct list * l1 = alloc(struct list);
    struct list * l2 = alloc(struct list);
    struct list * l3 = alloc(struct list);

    l0->data = 1;
    l1->data = 2;
    l2->data = 3;
    l3->data = 4;

    l0->next = l1;
    l1->next = l2;
    l2->next = l3;
    l3->next = NULL;

    int sum = 0;
    for(struct list * l = l0; l != NULL; l = l->next)
    {
        sum += l->data;
    }

    return sum;
}

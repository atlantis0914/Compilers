struct list {
  int head;
  struct list* tail;
};

typedef struct list list_t;

list_t* upto(int n) {
  list_t * p;
  list_t * q;
  q = NULL;
  while (n > 0) {
    p = alloc(list_t);
    p->head = n;
    p->tail = q;
    q = p;
    n -= 1;
  }
  return q;
}

int sum (list_t *p) {
  int sum = 0;
  while (p != NULL) {
    sum += p->head;
    p = p->tail;
  }
  return sum;
}

int main () {
  list_t *p;
  p = NULL;
  p = upto(100);
  return sum(p);
}

// some more complicated expressions

int main() {
  int x;
  int y;

  x = 1 + 2 + 3 + 4 % 3;  // 7
  y = 5 / 2 * 7 - 1;      // 13
  x *= y / (5 - (-1));    // 14
  x %= 5 + 4 * 3 / 2 - 1; // 4
  x = x / 2 * y + y - x;  // 35
  x %= 7 % 4;             // 2
  y /= 3 * x - x * x;     // 6
  x -= y;                 // -4

  return x * -1 + 2 - y;
}
